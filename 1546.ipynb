    "n = int(input())\n",
    "sung = [0 for x in range(n)]\n",
    "m = 0\n",
    "sum = 0\n",
    "ave = 0\n",
    "\n",
    "sung = list(map(int,input().split()))\n",
    "\n",
    "for i in range(n):\n",
    "    if m <= sung[i]:\n",
    "        m = sung[i]\n",
    "    \n",
    "for i in range(n):\n",
    "    sum = sum + (sung[i] / m * 100)\n",
    "\n",
    "ave = sum / n\n",
    "\n",
    "print(ave)"
